{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/dashboard/",
      "destination": "/dashboard"
    },
    {
      "source": "/login/",
      "destination": "/login"
    },
    {
      "source": "/register/",
      "destination": "/register"
